MODULE main
VAR
  p : boolean;
ASSIGN
  next(p) := case
    p : TRUE;
    TRUE : {TRUE,FALSE};
  esac;

SPEC EF p -> EG p

SPEC AF !p -> ! EG p
SPEC ! EG p -> AF !p

SPEC EF !p -> ! AF p
SPEC ! AF p -> EF !p

SPEC TRUE -> (AG p -> EG p)
SPEC (AG p -> EG p) -> TRUE

SPEC TRUE -> (EG p -> AG p)
SPEC (EG p -> AG p) -> TRUE